    "node-emoji": "^1.8.1",
    "ora": "^1.4.0",
    "request-promise": "^4.2.2",
    "stringz": "^0.4.0",
    "supports-hyperlinks": "^1.0.1",
    "update-notifier": "^2.3.0",
