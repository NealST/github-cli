    "@types/inquirer": "0.0.36",
    "@types/node": "^8.9.4",
    "@types/ora": "^1.3.2",
    "@types/update-notifier": "^2.0.0",
    "@types/node-emoji": "^1.8.0",
    "jest": "^22.4.2",
